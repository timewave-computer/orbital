                "type": "object",
                "required": [
                  "channel_id",
                  "connection_id",
                  "timeout"
                ],
                "properties": {
                  "channel_id": {
                    "type": "string"
                  },
                  "connection_id": {
                    "type": "string"
                  },
                  "timeout": {
                    "$ref": "#/definitions/Uint64"
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          }
        ]
